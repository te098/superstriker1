            <kind>1</kind>
            <string>/// Movimiento del personaje

// obtener las entradas de teclado
key_right = keyboard_check(vk_right);
key_left = keyboard_check(vk_left);
key_up = keyboard_check_pressed (vk_up);

//Operaciones con las entradas de teclado
move = key_right - key_left;
hsp = move * movespeed;

//colisión horizontal
if (place_meeting(x+hsp, y, obj_muros_padre)){
    while(!place_meeting(x+sign(hsp), y, obj_muros_padre)){
      x += sign(hsp);
  }
  hsp = 0;
}

x += hsp;

if (vsp &lt; 10) {vsp += grav;}

if (place_meeting(x, y+1, obj_muros_padre)){
   vsp = key_up * -jumpspeed;  
}

//colisión vertical
if (place_meeting(x, y+vsp, obj_muros_padre)){
    while(!place_meeting(x, y+sign(vsp), obj_muros_padre)){
      y += sign(vsp);
  }
  vsp = 0;
}
y += vsp;

//cambio sprites
if keyboard_check_pressed (vk_right){
 sprite_index = spr_br_walk_right;
}
if keyboard_check_released (vk_right){
 sprite_index = bruno_stay;
 }
 if keyboard_check_pressed (vk_left){
 sprite_index = spr_br_walk_left;
}
if keyboard_check_released (vk_left){
 sprite_index = bruno_stay;
 }
 if keyboard_check_pressed (vk_up){
 sprite_index = spr_br_jump_right;
}
if keyboard_check_released (vk_up){
 sprite_index = spr_br_fall_right;
 }
</string>
